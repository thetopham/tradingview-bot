# ProjectX / Topstep credentials
PROJECTX_BASE_URL=https://gateway-api-demo.s2f.projectx.com
PROJECTX_USERNAME=your_username
PROJECTX_API_KEY=your_api_key

# Flask webhook port
TV_PORT=5000

# Trading accounts (ACCOUNT_<NAME>=<ID>)
ACCOUNT_ALPHA=123456
# ACCOUNT_BETA=234567

# Per-account AI webhooks (N8N_AI_URL_<ACCOUNT>)
N8N_AI_URL_ALPHA=https://your-n8n.example.com/webhook/ai-alpha
# N8N_AI_URL_BETA=https://your-n8n.example.com/webhook/ai-beta

# Default symbol and contract overrides
DEFAULT_SYMBOL=MES
USE_DYNAMIC_CONTRACTS=true
LIVE_MODE=false
# OVERRIDE_CONTRACT_ID=CON.F.US.MESH24

# Risk limits
MAX_DAILY_LOSS=-500.0
DAILY_PROFIT_TARGET=500.0
# Set to 0 to disable the consecutive loss guard entirely
MAX_CONSECUTIVE_LOSSES=3

# Legacy stop/TP settings (kept for compatibility, not used by server-side brackets)
STOP_LOSS_POINTS=10.0
TP_POINTS=2.5,5.0,10.0

# Server-side bracket selection
# Default bracket template name; overridden per account via BRACKET_TEMPLATE_<ACCOUNT>
BRACKET_TEMPLATE=default
# BRACKET_TEMPLATE_ALPHA=aggressive

# Timeframes passed to the AI (comma-separated)
AI_TIMEFRAMES=5m,15m,30m,1h,4h,1D

# Supabase (for OHLC cache and trading logs)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_key

# TradingView webhook guard
WEBHOOK_SECRET=change_me
